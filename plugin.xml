            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="cz.vutbr.fit.xhriba01.bc.eclipse.editors.BytecodeEditor"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.editorss">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
